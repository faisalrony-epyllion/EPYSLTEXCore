@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="container-fluid">
    <div class="row">
        <div class="col-12" style="padding-right:0px !important;padding-left: 0px !important;">
            <div class="box">
                <div class="box-body">
                    <div class="nav-tabs-custom" style="position: relative;">
                        <button type="button" class="nav-link bg-light menu-move-btn" id="btnRightMenu" style="float:left;">
                            <i class="fas fa-angle-double-left"></i>
                        </button>
                        <button type="button" class="nav-link bg-light menu-move-btn" id="btnLeftMenu" style="right:0;">
                            <i class="fas fa-angle-double-right"></i>
                        </button>
                        <div class="menu-box" style="margin-left: 46px;">
                            <ul class="nav nav-tabs" id="mainTab">
                                <li class="p-2 active text-sm tabLi bg-info" tabIndex="0" style="box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);"><a href="#dashboard" data-toggle="tab" aria-expanded="true" style="margin-top: 3px;line-height: 30px;">Dashboard</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="tab-content" id="mainTabContent">
                        <div class="tab-pane active" id="dashboard"></div>
                    </div>
                </div>
                <!-- /.box-body -->
            </div>
        </div>
    </div>


</section>
